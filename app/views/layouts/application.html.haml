!!!
%html{:lang => "en"}
  %head
    %title Cottonwood Coding
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge", "http-equiv" => "X-UA-Compatible"}
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}
    %meta{:content => "", :name => "description"}
    %meta{:content => "", :name => "author"}
    %title Grayscale - Start Bootstrap Theme
    %link{:href => "bootstrap.min.css", :rel => "stylesheet"}
    %link{:href => "grayscale.css", :rel => "stylesheet"}
    %link{:href => "http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic", :rel => "stylesheet", :type => "text/css"}
    %link{:href => "http://fonts.googleapis.com/css?family=Montserrat:400,700", :rel => "stylesheet", :type => "text/css"}
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
  %body#page-top{"data-spy" => "scroll", "data-target" => ".navbar-fixed-top"}
    = yield
    %script{:src => "bootstrap.min.js"}
    %script{:src => "jquery.easing.min.js"}
    %script{:src => "grayscale.js"}